{
  "swagger": "2.0",
  "info": {
    "title": "Cartoon Management API",
    "description": "API for managing cartoons",
    "version": "1.0"
  },
  "basePath": "/",
  "schemes": ["http"],
  "paths": {
    "/": {
      "get": {
        "summary": "Home",
        "description": "Home endpoint",
        "responses": {
          "200": {
            "description": "Successful operation"
          }
        }
      }
    },
    "/register": {
      "post": {
        "summary": "Register user",
        "description": "Endpoint to register a new user",
        "parameters": [
          {
            "name": "username",
            "in": "body",
            "description": "Username of the user",
            "required": true,
            "schema": {
              "type": "string"
            }
          },
          {
            "name": "password",
            "in": "body",
            "description": "Password of the user",
            "required": true,
            "schema": {
              "type": "string"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "Successful registration",
            "schema": {
              "type": "object",
              "properties": {
                "message": {
                  "type": "string"
                }
              }
            }
          },
          "401": {
            "description": "Unauthorized"
          }
        }
      }
    },
    "/register_author": {
      "post": {
        "summary": "Register author",
        "description": "Endpoint to register a new author",
        "parameters": [
          {
            "name": "author_name",
            "in": "body",
            "description": "Name of the author",
            "required": true,
            "schema": {
              "type": "string"
            }
          },
          {
            "name": "username",
            "in": "body",
            "description": "Username of the author",
            "required": true,
            "schema": {
              "type": "string"
            }
          },
          {
            "name": "password",
            "in": "body",
            "description": "Password of the author",
            "required": true,
            "schema": {
              "type": "string"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "Successful registration",
            "schema": {
              "type": "object",
              "properties": {
                "message": {
                  "type": "string"
                }
              }
            }
          },
          "401": {
            "description": "Unauthorized"
          }
        }
      }
    },
    "/login": {
      "post": {
        "summary": "Login user",
        "description": "Endpoint to login user",
        "parameters": [
          {
            "name": "username",
            "in": "body",
            "description": "Username of the user",
            "required": true,
            "schema": {
              "type": "string"
            }
          },
          {
            "name": "password",
            "in": "body",
            "description": "Password of the user",
            "required": true,
            "schema": {
              "type": "string"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "Successful login"
          },
          "401": {
            "description": "Unauthorized"
          }
        }
      }
    },
    "/logout": {
      "post": {
        "summary": "Logout user",
        "description": "Endpoint to logout user",
        "parameters": [
          {
            "name": "username",
            "in": "body",
            "description": "Username of the user",
            "required": true,
            "schema": {
              "type": "string"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "Logout successful",
            "schema": {
              "type": "object",
              "properties": {
                "message": {
                  "type": "string"
                }
              }
            }
          },
          "401": {
            "description": "Unauthorized"
          }
        }
      }
    },
    "/get_cartoon": {
      "get": {
        "summary": "Get cartoon",
        "description": "Endpoint to get cartoon details",
        "parameters": [
          {
            "name": "cartoon",
            "in": "query",
            "description": "Name of the cartoon",
            "required": true,
            "schema": {
              "type": "string"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "Successful operation"
          }
        }
      }
    },
    "/all_cartoon": {
      "get": {
        "summary": "Get all cartoons",
        "description": "Endpoint to get all cartoons",
        "responses": {
          "200": {
            "description": "Successful operation"
          }
        }
      }
    },
    "/all_cartoon_category": {
      "get": {
        "summary": "Get all cartoons by category",
        "description": "Endpoint to get all cartoons by category",
        "parameters": [
          {
            "name": "category",
            "in": "query",
            "description": "Name of the category",
            "required": true,
            "schema": {
              "type": "string"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "Successful operation"
          }
        }
      }
    },
    "/post_cartoon": {
      "post": {
        "summary": "Post cartoon",
        "description": "Endpoint to post a new cartoon",
        "consumes": ["multipart/form-data"],
        "parameters": [
          {
            "name": "name_cartoon",
            "in": "formData",
            "description": "Name of the cartoon",
            "required": true,
            "type": "string"
          },
          {
            "name": "author",
            "in": "formData",
            "description": "Author of the cartoon",
            "required": true,
            "type": "string"
          },
          {
            "name": "category",
            "in": "formData",
            "description": "Category of the cartoon",
            "required": true,
            "type": "array",
            "items": {
              "type": "string"
            },
            "collectionFormat": "multi"
          },
          {
            "name": "image_cartoon",
            "in": "formData",
            "description": "Image file of the cartoon",
            "required": true,
            "type": "file"
          },
          {
            "name": "image_main",
            "in": "formData",
            "description": "Main image file of the cartoon",
            "required": true,
            "type": "file"
          },
          {
            "name": "image_background",
            "in": "formData",
            "description": "Background image file of the cartoon",
            "required": true,
            "type": "file"
          }
        ],
        "responses": {
          "200": {
            "description": "Successful operation"
          },
          "401": {
            "description": "Unauthorized"
          }
        }
      }
    },
    "/post_chapter": {
      "post": {
        "summary": "Post chapter",
        "description": "Endpoint to post a new chapter",
        "consumes": ["multipart/form-data"],
        "parameters": [
          {
            "name": "name_cartoon",
            "in": "formData",
            "description": "Name of the cartoon",
            "required": true,
            "type": "string"
          },
          {
            "name": "name_chapter",
            "in": "formData",
            "description": "Name of the chapter",
            "required": true,
            "type": "string"
          },
          {
            "name": "coin",
            "in": "formData",
            "description": "Coin required for the chapter",
            "required": true,
            "type": "integer",
            "format": "int32"
          },
          {
            "name": "files[]",
            "in": "formData",
            "description": "Chapter image files",
            "required": true,
            "type": "file",
            "collectionFormat": "multi"
          }
        ],
        "responses": {
          "200": {
            "description": "Files uploaded successfully",
            "schema": {
              "type": "object",
              "properties": {
                "message": {
                  "type": "string"
                }
              }
            }
          }
        }
      }
    },
    "/get_category": {
      "get": {
        "summary": "Get category",
        "description": "Endpoint to get category",
        "responses": {
          "200": {
            "description": "Successful operation"
          }
        }
      }
    },
    "/get_chapter": {
      "get": {
        "summary": "Get chapter",
        "description": "Endpoint to get chapter details",
        "parameters": [
          {
            "name": "cartoon",
            "in": "query",
            "description": "Name of the cartoon",
            "required": true,
            "type": "string"
          },
          {
            "name": "chapter",
            "in": "query",
            "description": "Chapter number",
            "required": true,
            "type": "string"
          }
        ],
        "responses": {
          "200": {
            "description": "Successful operation"
          }
        }
      }
    },
    "/get_user": {
      "get": {
        "summary": "Get user",
        "description": "Endpoint to get user details",
        "parameters": [
          {
            "name": "username",
            "in": "query",
            "description": "Username of the user",
            "required": true,
            "type": "string"
          }
        ],
        "responses": {
          "200": {
            "description": "Successful operation"
          }
        }
      }
    },
    "/get_author": {
      "get": {
        "summary": "Get author",
        "description": "Endpoint to get author details",
        "parameters": [
          {
            "name": "username",
            "in": "query",
            "description": "Username of the author",
            "required": true,
            "type": "string"
          }
        ],
        "responses": {
          "200": {
            "description": "Successful operation"
          }
        }
      }
    },
    "/buy_coin": {
      "post": {
        "summary": "Buy coin",
        "description": "Endpoint to buy coins",
        "parameters": [
          {
            "name": "username",
            "in": "body",
            "description": "Username of the user",
            "required": true,
            "schema": {
              "type": "string"
            }
          },
          {
            "name": "total_coin",
            "in": "body",
            "description": "Total coins of the user",
            "required": true,
            "schema": {
              "type": "integer",
              "format": "int32"
            }
          },
          {
            "name": "amount",
            "in": "body",
            "description": "Amount of coins to buy",
            "required": true,
            "schema": {
              "type": "integer",
              "format": "int32"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "Successful operation"
          }
        }
      }
    },
    "/buy_chapter": {
      "post": {
        "summary": "Buy chapter",
        "description": "Endpoint to buy chapter",
        "parameters": [
          {
            "name": "username",
            "in": "body",
            "description": "Username of the user",
            "required": true,
            "schema": {
              "type": "string"
            }
          },
          {
            "name": "cartoon_id",
            "in": "body",
            "description": "ID of the cartoon",
            "required": true,
            "schema": {
              "type": "string"
            }
          },
          {
            "name": "chapter_id",
            "in": "body",
            "description": "ID of the chapter",
            "required": true,
            "schema": {
              "type": "string"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "Successful operation"
          }
        }
      }
    }
  }
}
